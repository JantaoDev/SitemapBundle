{
    "name": "jantaodev/sitemap-bundle",
    "version": "0.2",
    "description": "Symfony bundle that provides sitemap XML generation",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "sitemap", "robots"],
    "authors": [
        {
            "name": "Sergey Hayevoy",
            "email": "jantao.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^5.0",
        "symfony/yaml": "*",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "symfony/security-csrf": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "symfony/browser-kit": "*",
        "symfony/mime": "*"
    },
    "autoload": {
        "psr-4": { "JantaoDev\\SitemapBundle\\": "" }
    },
    "scripts": {
        "test" : "./vendor/bin/simple-phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
