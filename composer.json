{
    "name": "jantaodev/sitemap-bundle",
    "description": "Symfony bundle that provides sitemap XML generation",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony", "sitemap", "robots"],
    "authors": [
        {
            "name": "Sergey Hayevoy",
            "email": "jantao.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/yaml": "*",
        "doctrine/orm": "^2.4",
        "doctrine/doctrine-bundle": "^1.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^2.7|^3.0",
        "symfony/security-csrf": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "symfony/browser-kit": "*"
    },
    "autoload": {
        "psr-4": { "JantaoDev\\SitemapBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    }
}
